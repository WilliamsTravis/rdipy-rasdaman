{
    "config": {
        "service_url": "http://localhost:8080/rasdaman/ows",
        "tmp_directory": "/tmp/",
        "mock": false,
        "automated": true,
        "track_files": false,
        "subset_correction": false
    },
    "input": {
        "coverage_id": "ppt",
        "paths": [
            "/data/ssri/ppt.nc"
        ]
    },
    "recipe": {
        "name": "general_coverage",
        "options": {
            "tiling": "ALIGNED [0:100, 0:100, 0:100] TILE SIZE 1000000",
            "coverage": {
                "crs": "OGC/0/AnsiDate@EPSG/0/4326",
                "metadata": {
                    "type": "xml",
                    "global": {}
                },
                "slicer": {
                    "type": "netcdf",
                    "pixelIsPoint": true,
                    "bands": [
                        {
                            "name": "PPT",
                            "identifier": "ppt"
                        }
                    ],
                    "axes": {
                        "ansi": {
                            "min": "${netcdf:variable:time:min}",
                            "max": "${netcdf:variable:time:max}",
                            "directPositions": "${netcdf:variable:time}",
                            "type": "ansidate",
                            "resolution": "2",
                            "irregular": false,
                            "crs_origin": "1900-01-01",
                            "gridOrder": 0
                        },
                        "Lat": {
                            "min": "${netcdf:variable:latitude:min}",
                            "max": "${netcdf:variable:latitude:max}",
                            "gridOrder": 1,
                            "resolution": 0.25
                        },
                        "Long": {
                            "min": "${netcdf:variable:longitude:min}",
                            "max": "${netcdf:variable:longitude:max}",
                            "gridOrder": 2,
                            "resolution": 0.25
                        }
                    }
                }
            }
        }
    }
}